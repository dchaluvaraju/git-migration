# GitLab Migration Configuration
# Copy this file to .env and fill in your actual values

# Source GitLab instance (Community Edition)
GITLAB_CE_URL=https://gitlab-ce.example.com

# Destination GitLab instance (Enterprise Edition)
GITLAB_EE_URL=https://gitlab-ee.example.com

# Access token for source GitLab (needs read_api and read_repository scopes)
GITLAB_CE_TOKEN=glpat-xxxxxxxxxxxx

# Access token for destination GitLab (needs api scope)
GITLAB_EE_TOKEN=glpat-yyyyyyyyyyyy

# Path to file containing list of repositories to migrate (one per line)
GITLAB_PROJECTS_FILE=./projects.txt

# Root group name in destination GitLab where migrated projects will be placed
GITLAB_DEST_ROOT_GROUP=migrated-repos
